<%=  form_for @autor, remote: true, html: { class: "new-autor" }  do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <hr />
  <%= f.label :surname %>
  <%= f.text_field :surname %>
  <hr />
  <%= f.label :age %>
  <%= f.number_field :age, class:'text-center'%>
  <hr />
  <%= f.submit 'save', role: 'button', class:'btn btn-outline-success' %>
<% end %>
